{
    "extends": "./node_modules/@englercj/code-style/.eslintrc.json",
    "env": {
        "node": false,
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "brace-style": [2, "allman"],
        "no-magic-numbers": 0,
        "spaced-comment": [2, "always", { "exceptions": ["?"] }]
    }
}
