{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "./plugins"
        ],
        "exclude": [
            "./plugins/**/test/**"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "./node_modules/jsdoc/plugins/markdown",
        "./build/jsdoc-fix"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "Fae",
        "copyright": "Fae Copyright Â© 2016 Chad Engler.",
        "linenums": true
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "private": true,
        "lenient": true,
        "destination": "./api-docs",
        "template": "./node_modules/jaguarjs-jsdoc"
    }
}
